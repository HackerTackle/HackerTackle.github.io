{
  "name": "HackerTackle.github.io",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "build-task:scss-compile": "node-sass-chokidar --source-map true stylesheets/sass/ -o stylesheets/css/",
    "build-task:autoprefixer": "postcss stylesheets/css/*.css --use autoprefixer -d stylesheets/css/",
    "sass:build": "npm-run-all -p build-task:*",
    "sass:watch": "chokidar 'stylesheets/sass/*.scss' -c 'npm run sass:build'",
    "http-server": "http-server",
    "dev": "npm-run-all -p sass:*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HackerTackle/HackerTackle.github.io.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/HackerTackle/HackerTackle.github.io/issues"
  },
  "homepage": "https://github.com/HackerTackle/HackerTackle.github.io#readme",
  "devDependencies": {
    "autoprefixer": "^7.2.1",
    "chokidar-cli": "^1.2.0",
    "http-server": "^0.9.0",
    "node-sass": "^3.8.0",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.2",
    "postcss-cli": "^4.1.1",
    "vue": "^2.5.10",
    "vue-resource": "^1.3.4",
    "vunit.js": "^0.2.0"
  }
}
